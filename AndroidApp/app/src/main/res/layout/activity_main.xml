<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/ic_heart_box_70dp"
            android:tint="@color/colorPrimary" android:background="@color/zxing_transparent"
            android:id="@+id/start_feedpack_btn" android:layout_marginTop="80dp"
            app:layout_constraintTop_toBottomOf="@+id/textView11" android:layout_marginStart="84dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="FeedPack"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            android:fontFamily="@font/open_sans_semibold" android:textColor="@color/colorPrimary"
            app:layout_constraintTop_toBottomOf="@+id/start_feedpack_btn"
            app:layout_constraintEnd_toEndOf="@+id/start_feedpack_btn"
            app:layout_constraintStart_toStartOf="@+id/start_feedpack_btn"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/ic_rating_70dp"
            android:tint="@color/colorPrimary" android:background="@color/zxing_transparent"
            android:id="@+id/imageButton4"
            app:layout_constraintTop_toTopOf="@+id/start_feedpack_btn" app:layout_constraintStart_toEndOf="@+id/start_feedpack_btn"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="Investigation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView8"
            android:fontFamily="@font/open_sans_semibold" android:textColor="@color/colorPrimary"
            app:layout_constraintTop_toBottomOf="@+id/imageButton4"
            app:layout_constraintEnd_toEndOf="@+id/imageButton4"
            app:layout_constraintStart_toStartOf="@+id/imageButton4"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/ic_warning_70dp"
            android:tint="@color/colorPrimary" android:background="@color/zxing_transparent"
            android:id="@+id/imageButton5" android:layout_marginTop="64dp"
            app:layout_constraintTop_toBottomOf="@+id/textView6"
            app:layout_constraintStart_toStartOf="@+id/start_feedpack_btn"/>
    <TextView
            android:text="CASY"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView9"
            android:fontFamily="@font/open_sans_semibold" android:textColor="@color/colorPrimary"
            app:layout_constraintTop_toBottomOf="@+id/imageButton5"
            app:layout_constraintEnd_toEndOf="@+id/imageButton5"
            app:layout_constraintStart_toStartOf="@+id/imageButton5"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/ic_shopping_cart_70dp"
            android:tint="@color/colorPrimary" android:background="@color/zxing_transparent"
            android:id="@+id/imageButton6"
            app:layout_constraintTop_toTopOf="@+id/imageButton5"
            app:layout_constraintEnd_toEndOf="@+id/imageButton4"/>
    <TextView
            android:text="Sales
"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView10"
            android:fontFamily="@font/open_sans_semibold" android:textColor="@color/colorPrimary"
            app:layout_constraintTop_toBottomOf="@+id/imageButton6"
            app:layout_constraintEnd_toEndOf="@+id/imageButton6"
            app:layout_constraintStart_toStartOf="@+id/imageButton6"/>
    <TextView
            android:text="What do you want to do?"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView11"
            android:fontFamily="@font/open_sans_semibold" android:textColor="@color/colorPrimary"
            android:textSize="24sp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="64dp" app:layout_constraintTop_toTopOf="parent"/>

</android.support.constraint.ConstraintLayout>